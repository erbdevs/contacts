<h1>List of contacts</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Link</th>
    </tr>
  </thead>
  <tbody>
    <% @all_contacts.each do |contact| %>
      <tr>
        <td><%= contact.first_name %></td>
        <td><%= contact.last_name %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.email %></td>
        <td><%= link_to("See more", contact)%></td>
      </tr>
    <% end %>
  </tbody>
</table>
