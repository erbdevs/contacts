<h1>Historical changes for <%= @contact.first_name %></h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Update Date</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @contact.first_name %></td>
      <td><%= @contact.last_name %></td>
      <td><%= @contact.phone %></td>
      <td><%= @contact.email %></td>
      <td><%= @contact.updated_at %></td>
    </tr>
    <% @all_versions.each do |version| %>
      <tr>
        <td><%= version.first_name %></td>
        <td><%= version.last_name %></td>
        <td><%= version.phone %></td>
        <td><%= version.email %></td>
        <td><%= version.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
